{
  "name": "secure-tunnel",
  "private": true,
  "version": "4.5.8",
  "type": "module",
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "build:inline": "npx tsx build-inline.ts",
    "analyze:deadcode": "knip --reporter json",
    "preview": "vite preview --host --port 80",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --max-warnings=0",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,css,md,json}\"",
    "analyze:bundle": "cross-env ANALYZE=true vite build",
    "test": "vitest",
    "test:ci": "vitest --run",
    "prepare": "husky install"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.0",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.0",
    "@vitejs/plugin-react": "^4.6.0",
    "knip": "^5.76.3",
    "tsx": "^4.20.5",
    "typescript": "^5.5.0",
    "vite": "^7.0.0",
    "eslint": "^8.40.0",
    "prettier": "^2.8.8",
    "@typescript-eslint/parser": "^6.2.0",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-config-prettier": "^8.8.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "cross-env": "^7.0.3",
    "vitest": "^1.4.4",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.0"
  }
}
